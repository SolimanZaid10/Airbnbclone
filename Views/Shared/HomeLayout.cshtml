    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/font-awesome/4.4.0/css/font-awesome.min.css">


    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/jquery-validation/dist/jquery.validate.js"></script>
    <script src="~/lib/jquery-validation-unobtrusive/jquery.validate.unobtrusive.js"></script>
    <script src="~/js/jquery.unobtrusive-ajax.min.js"></script>
    <script src="https://cdn.jsdelivr.net/npm/@@popperjs/core@2.9.1/dist/umd/popper.min.js"
            integrity="sha384-SR1sx49pcuLnqZUnnPwx6FCym0wLsk5JZuNx2bPPENzswTNFaQU1RDvt3wT4gWFG"
            crossorigin="anonymous"></script>
    <script type="text/javascript" src="https://cdn.jsdelivr.net/jquery/latest/jquery.min.js"></script>
    <script type="text/javascript" src="https://cdn.jsdelivr.net/momentjs/latest/moment.min.js"></script>
    <script type="text/javascript" src="https://cdn.jsdelivr.net/npm/daterangepicker/daterangepicker.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    <!--Logic for Glow Button-->
    <script src="~/js/glow.js"></script>
    <script>
        var numberOfAdults = 0
        var numberOfChildren = 0
        var numberOfInfants = 0
        var numberOfGuests = numberOfAdults + numberOfChildren

        $(function () {
            $('.guests').css('display', 'none')
            $('.location').css('display', 'none')
            $('#bgmodal').css('display', 'none')
            $('#Guest_Num_Navbar').html("Add Guests")
            $('.number_Adults').html(numberOfAdults)

            $('#calender').addClass("focus")
            $(document).mouseup(function (e) {
                var datepickerr = $('.daterangepickerr')
                var Guests = $('.guests')
                var location = $('.location')
                var bgmodal = $('#bgmodal')
                if (!datepickerr.is(e.target) && datepickerr.has(e.target).length === 0 &&
                    !Guests.is(e.target) && Guests.has(e.target).length === 0 &&
                    !location.is(e.target) && location.has(e.target).length === 0
                ) {
                    bgmodal.hide()
                }
                else {
                    bgmodal.css('display', 'block !important')
                }
                if (!datepickerr.is(e.target) && datepickerr.has(e.target).length === 0) {
                    datepickerr.hide()

                } else {
                    datepickerr.css('display', 'block !important')
                }
                if (!Guests.is(e.target) && Guests.has(e.target).length === 0) {
                    Guests.hide()

                } else {
                    Guests.css('display', 'block !important')
                }
                if (!location.is(e.target) && location.has(e.target).length === 0) {
                    location.hide()

                } else {
                    location.css('display', 'block !important')
                }
            })
        });
        $('.stay_for_buttons button').click(function (e) {
            if (!$('.stay_for_Added').length) {
                $('#stay_for_span').append('<span class="stay_for_Added">' + e.target.textContent + '</span>')
                $(this).addClass("stay_for_focus")
            }
            else {
                $('.stay_for_buttons button').removeClass("stay_for_focus")
                $('.stay_for_Added').remove()
                $('#stay_for_span').append('<span class="stay_for_Added">' + e.target.textContent + '</span>')
                $(this).addClass("stay_for_focus")
            }
        })
        $('#check-in').daterangepicker({
            opens: 'center',
            autoUpdateInput: false,
            isInvalidDate: (date) => {
                $('.cancelBtn').text('Clear');
                return (new Date() >= date._d);
            },
        });
        $('#check-in').on('apply.daterangepicker', function (ev, picker) {
            var checkin = picker.startDate.format('YYYY-MM-DD');
            var checkout = picker.endDate.format('YYYY-MM-DD');

            $('#check-in').val(checkin);
            $('#check-out').val(checkout);
        });

        $('#check-in').on('cancel.daterangepicker', function (ev, picker) {
            $('#check-in, #check-out').val('');
        });
        $('#check-out').on('focus', () => {
            $('#check-in').focus();
        });


        //$('input[name="daterange"]').click(function () {
        //    $('.daterangepickerr').append($('.daterangepicker'))
        //    $('#calender').addClass('focus')
        //    $('#flexible').removeClass('focus')
        //    $('.daterangepicker').addClass('show')
        //    $('.daterangepickerr').css('display', 'block')
        //    $('#bgmodal').css('display', 'block')
        //})



        $('#calender').click(function () {
            $('#calender').addClass('focus')
            $('#flexible').removeClass('focus')
            $('.daterangepicker').css('display', 'block')
            $('.flexible_container').css('display', 'none')
        })
        $('.go_in_buttons div button').click(function (e) {
            console.log(e)
            dataAdded = '<span class="stay_for_Added">' + e.target.textContent + '</span>';
            if ($(this).hasClass('go_in_buttons_focus')) {
                // if($('.stay_for_Added').text===e.target.textContent){
                //     $('.stay_for_Added').remove()
                $(this).removeClass('go_in_buttons_focus')
                //}
            } else {

                $('.go_in_span').append(dataAdded)
                $(this).addClass('go_in_buttons_focus')

            }
        })
        $('#flexible').click(function () {
            $('.daterangepicker').removeClass('show')
            $('#calender').removeClass('focus')
            $('#flexible').addClass('focus')
            $('.daterangepicker').css('display', 'none')
            $('.flexible_container').css('display', 'block')
        })
        // $('#searchAreaSmall').click(function () {
        //     $('#SearchTxtSmall').addClass('smallsearchbar')
        //     $('.contain-Sm-scr').append($('.location'))
        // })
        $('.table-condensed tbody tr td').css('background-color', 'red')

        $('#Guest_Num_Navbar').click(function () {
            $('.guests').css('display', 'block')
            $('#bgmodal').css('display', 'block')
        })
        $('.Select_Location_Nav_Bar').click(function () {
            $('.location').css('display', 'block')
            $('#bgmodal').css('display', 'block')

        })


        $('.add_Adults').click(function () {

            numberOfAdults++
            numberOfGuests++
            $('#Guest_Num_Navbar').html(numberOfGuests + "guests")

            $('.number_Adults').html(numberOfAdults)
            $('#clear_btn').removeAttr("disabled")
            $('#clear_btn').removeClass("disabled")

            $('.subtract_Adults').removeAttr("disabled")
        })
        $('.subtract_Adults').click(function () {

            if (numberOfAdults == 0) {
                $('.subtract_Adults').attr("disabled")
            } else if (numberOfAdults == 1) {
                numberOfAdults--
                numberOfGuests--
                $('.number_Adults').html(numberOfAdults)
                if (numberOfGuests == 0) {
                    $('#Guest_Num_Navbar').html("Add guests")
                } else if (numberOfGuests != 0) {
                    $('#Guest_Num_Navbar').html(numberOfGuests)
                }
            }
            else {
                numberOfGuests--;
                numberOfAdults--
                $('.number_Adults').html(numberOfAdults)
                if (numberOfInfants == 0) {
                    $('#Guest_Num_Navbar').html(numberOfGuests)
                }
            }
        })

        $(window).scroll(function () {
            st = window.pageYOffset
            if (st > 100) {
                $("#mainnav").css('background-color', 'white')
                $('#searchAreaSmall').css('background-color', 'white')
                $('#searchAreaSmall').addClass('fixed_nav_bar')
                $('#mainnav').addClass('fixed_nav_bar')
                $('#logoAirbnb').css('color', '#FF385C')
                $('#logo_without_Word').css('color', '#FF385C')
                $('#becomeahost').css({ 'background-color': 'white', 'color': 'black', 'border': '1px solid black' })
                $('#optionUser').css('border', '1px solid lightgray')
                $('#language_Icon').css('color', 'black')

            }
            else {
                $("#mainnav").css('background-color', 'black')
                $('#mainnav').removeClass('fixed_nav_bar')
                $('#searchAreaSmall').css('background-color', 'black')
                $('#searchAreaSmall').removeClass('fixed_nav_bar')
                $('#logoAirbnb').css('color', 'white')
                $('#logo_without_Word').css('color', 'white')
                $('#becomeahost').css({ 'background-color': 'black', 'color': 'white', 'border': '1px solid white' })
                $('#language_Icon').css('color', 'white')
            }
        })

        // small  search screen
        $('#SearchTxtSmall').click(function () {
            $('#divLocation').css('display', 'block')

            $('#locationSmallDiv').css('display', 'block')
            $('#searchAreaSmall').addClass("disappear");
            $('#containSmScr').css("display", "block").append($('#divLocation')
                .removeClass("location").addClass("location_sm"));
            $('#containSmScr').append($('#searchArea_Sm').css("display", "block"));

        });


        //Small screen
        //click on location
        $('#divLocation').daterangepicker({
            opens: 'left',
            autoUpdateInput: false,
            isInvalidDate: (date) => {
                $('.cancelBtn').text('Clear');
                return (new Date() >= date._d);
            },
        });

        $('#divLocation').click(function () {
            $('#locationSmallDiv').css('display', 'none')
            $('.NextBtn').css('display', 'block')
            $('#calender').addClass('focus')
            $('#flexible').removeClass('focus')
            //$('.daterangepicker:eq(1)').css( 'left', '0px !important' )

            //$('.daterangepicker', 1).css({ 'left': '10px !important'}, { 'width':'400px !important'})

            $('#bgmodal').css('display', 'block')
        });
        $('.NextBtn').click(function () {
            $('.NextBtn').css('display', 'none');
            $('.daterangepicker').removeClass('show')
            $('#divLocation').css('display', 'none !important');
            $('#containSmScr').append($('#containTopButton').css("display", "flex"));
            //$('#containSmScr').append($('<h1>Add guests</h1>').css("display", "block"));
            $(".guests_sm_src").css("display", "block")

            // $('#containSmScr').append($('.guests').css("display", "block").removeClass("guests").addClass("guests_sm_src"));
            $('#containSmScr').append($('#containLowerButton').css("display", "flex"));
            $('#clear_btn').addClass("disabled");
        })



        // $('#divLocation').click(function () {
        //     $('input[name="daterange"]').click()
        //     // $('#searchArea_Sm').remove();
        //     // $('#divLocation').remove();
        //     // $('#containSmScr').append($('#containTopButton').css("display", "block"));
        //     // //$('#containSmScr').append($('<h1>Add guests</h1>').css("display", "block"));
        //     // $(".guests_sm_src").css("display", "block")
        //     // // $('#containSmScr').append($('.guests').css("display", "block").removeClass("guests").addClass("guests_sm_src"));
        //     // $('#containSmScr').append($('#containLowerButton').css("display", "block"));
        //     // $('#clear_btn').addClass("disabled");

        // });
        $('#cancel_SM').click(function () {
            $('.contain-Sm-scr ').css("display", "none")
            $(".guests_sm_src").css("display", "none")
            $("#containLowerButton").css("display", "none")
            $("#containTopButton").css("display", "none")


            $('#searchAreaSmall').removeClass("disappear");
        })
        //click on clear button
        $('#clear_btn').click(function () {
            console.log("clicked")
            $('#Skip_btn').removeClass("disabled")
            $('.number_Adults').html(numberOfAdults = 0)
            $('.subtract_Adults').addClass("disabled")
            $('#clear_btn').addClass("disabled")
        });
        [].forEach.call(document.getElementsByClassName('glow'), (el) => {
            el.onmousemove = function moveEvent(e) {
                // e = Mouse click event.
                var rect = e.target.getBoundingClientRect();
                var x = e.clientX - rect.left; //x position within the element.
                var y = e.clientY - rect.top;  //y position within the element.
                let glowing = e.target;
                glowing.style.backgroundImage = `radial-gradient(circle at top ${y}px left ${x}px, #FF385C 0%, #E61E4D 27.5%, #E31C5F 40%, #D70466 57.5%, #BD1E59 75%, #BD1E59 100%)`;
            }
        });
        function fetchStates() {
            var id = $('#countries').val();

            $.ajax({
                type: "GET",
                url: `/listing/states/${id}`,
                success: function (response) {
                    $('#states').html(response);
                }
            });
        }

        function fetchCities() {
            var id = $('#statesSelect').val();

            $.ajax({
                type: "GET",
                url: `/listing/cities/${id}`,
                success: function (response) {
                    $('#cities').html(response);
                    setMapCenter();
                    $('#map').show();
                }
            });
        }

        function setMapCenter() {
            var selectCity = $('#citySelect').get()[0];
            var selectedCity = selectCity.options[selectCity.selectedIndex].text;

            $('#cityName').val(selectedCity);
        }

        $('#search').click((e) => {
            nOfGuests = $('#Guest_Num_Navbar').html();

            nOfGuests = nOfGuests.replace('guests', '');

            if (!isNaN(Number(nOfGuests))) {
                var nOfGuests = `<input name="NoOfGuests" value="${nOfGuests}" style="display: none;" />`
                $('#search-form').append(nOfGuests);
            }
        });
    </script>
    @await RenderSectionAsync("Scripts", required: false)

</body>
</html>